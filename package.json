{
  "name": "compoundeffect",
  "version": "1.0.0",
  "description": "To build a software system that demonstrates the **Compound Effect (Positive Reinforcement Loop)**, where every iteration, user interaction, and line of code contributes to an exponential increase in value and utility over time.",
  "main": "index.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "build": "tsc",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write \"src/**/*.{ts,json}\"",
    "dashboard": "ts-node src/server.ts",
    "prepare": "husky install"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/node": "^25.3.0",
    "better-sqlite3": "^12.6.2",
    "eslint": "^10.0.0",
    "husky": "^8.0.0",
    "lint-staged": "^16.2.7",
    "plop": "^4.0.5",
    "prettier": "^3.8.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18",
    "zod": "^4.3.6"
  },
  "dependencies": {
    "axios": "^1.13.5",
